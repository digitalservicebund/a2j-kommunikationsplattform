{
  "name": "a2j-kommunikationsplattform",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "NODE_OPTIONS='--import ./app/instrument.server.mjs' node ./server.js",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS='--import ./app/instrument.server.mjs' node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:generate-coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:a11y": "playwright test --config=tests/a11y/playwright.config.ts",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow '' --summary"
  },
  "dependencies": {
    "@digitalservice4germany/angie": "^2.0.1",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@digitalservicebund/icons": "^2.0.1",
    "@kern-ux/native": "^2.2.0",
    "@mjackson/file-storage": "^0.7.0",
    "@mjackson/form-data-parser": "^0.9.1",
    "@react-router/express": "^7.7.0",
    "@react-router/fs-routes": "^7.7.0",
    "@react-router/node": "^7.7.0",
    "@react-router/serve": "^7.7.0",
    "@sentry/react-router": "^9.40.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/expect": "^3.2.4",
    "compression": "^1.8.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "isbot": "^5.1.28",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.0",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@react-router/dev": "^7.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "license-checker": "^25.0.1",
    "msw": "^2.10.4",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-router-devtools": "^5.0.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
