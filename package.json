    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.36.0",
