{
  "name": "a2j-kommunikationsplattform",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "node ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:generate-coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:a11y": "playwright test --config=tests/a11y/playwright.config.ts",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "preinstall": "echo 'Please disable script execution: npm config set ignore-scripts true' && exit 1",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix"
  },
  "dependencies": {
    "@kern-ux/native": "^2.3.2",
    "@react-router/express": "^7.11.0",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@sentry/react-router": "^10.22.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "isbot": "^5.1.31",
    "morgan": "^1.10.1",
    "pino-http": "^11.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "react-router-dom": "^7.11.0",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@digitalservicebund/kern-ux-tailwind-theme": "^1.1.0",
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vitest/eslint-plugin": "^1.5.4",
    "@vitest/expect": "^4.0.15",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.6",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-router-devtools": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "overrides": {
    "glob": "11.1.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
