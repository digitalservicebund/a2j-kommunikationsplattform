    "@react-router/serve": "^7.8.2",
    "@sentry/react-router": "^10.14.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/expect": "^3.2.4",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cookie": "^1.0.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "isbot": "^5.1.30",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
