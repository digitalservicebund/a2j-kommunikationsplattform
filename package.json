{
  "name": "a2j-kommunikationsplattform",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "NODE_OPTIONS='--import ./app/instrument.server.mjs' node ./server.js",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS='--import ./app/instrument.server.mjs' node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:generate-coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:a11y": "playwright test --config=tests/a11y/playwright.config.ts",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow '' --summary"
  },
  "dependencies": {
    "@digitalservice4germany/angie": "^2.0.1",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@digitalservicebund/icons": "^2.0.1",
    "@mjackson/file-storage": "^0.6.1",
    "@mjackson/form-data-parser": "^0.7.0",
    "@react-router/express": "^7.5.0",
    "@react-router/fs-routes": "^7.5.0",
    "@react-router/node": "^7.4.1",
    "@react-router/serve": "^7.4.1",
    "@sentry/react-router": "^9.11.0",
    "@vitest/coverage-istanbul": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.0",
    "@vitest/expect": "^3.1.4",
    "compression": "^1.8.0",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "isbot": "^5.1.25",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.2",
    "remix-auth": "^4.1.0",
    "remix-auth-oauth2": "^3.4.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@react-router/dev": "^7.4.1",
    "@tailwindcss/vite": "^4.1.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "license-checker": "^25.0.1",
    "msw": "^2.7.5",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
